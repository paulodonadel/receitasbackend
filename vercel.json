{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://sistema-receitas-backend.onrender.com/:path*"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_URL": "https://sistema-receitas-backend.onrender.com"
  }
}
